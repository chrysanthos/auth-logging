{
    "name": "chrysanthos/auth-logging",
    "description": "Authentication logger for Laravel",
    "keywords": [
        "Laravel",
        "Laravel Framework",
        "Laravel Package",
        "Laravel Auth",
        "Laravel Authentication",
        "Laravel Admin",
        "PHP Package",
        "Auth Logging"
    ],
    "support": {
        "issues": "https://github.com/chrysanthos/auth-logging/issues",
        "source": "https://github.com/chrysanthos/auth-logging"
    },
    "homepage": "https://github.com/Chrysanthos/auth-logging",
    "license": "MIT",
    "authors": [
        {
            "name": "Chrysanthos Prodromou",
            "email": "pchrysanthos@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">7.3",
        "illuminate/support": "^6.5 | ^7.0 | ^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.3",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Chrysanthos\\AuthLogging\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chrysanthos\\AuthLogging\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chrysanthos\\AuthLogging\\AuthLoggingServiceProvider"
            ]
        }
    }
}
